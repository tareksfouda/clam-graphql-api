{
  "name": "clam-api",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MoonHighway/clam-graphql-api"
  },
  "scripts": {
    "format": "prettier --write '**.js'; prettier --write '**.md'; prettier --write '**.json';",
    "build": "webpack -p",
    "start": "node ./dist",
    "pretest": "npm run format",
    "test": "jest --coverage; npm run test:functional",
    "test:functional": "cucumber-js --require-module @babel/register --require-module @babel/preset-env --require-module babel-polyfill --require-module dotenv/config",
    "dev:start": "nodemon --exec babel-node ./src -e js,graphql",
    "dev:test": "jest --watch",
    "predev:functional": "clear",
    "dev:functional": "npm run test:functional -- -f node_modules/cucumber-pretty",
    "dev:coverage": "open ./coverage/lcov-report/index.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.1.0",
    "graphql": "^14.0.2",
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "apollo-boost": "^0.1.17",
    "apollo-client": "^2.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import-graphql": "^2.6.2",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "chai": "^4.2.0",
    "cucumber": "^5.0.2",
    "cucumber-pretty": "^1.4.4",
    "dotenv": "^6.1.0",
    "jest": "^23.6.0",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.12.1",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
